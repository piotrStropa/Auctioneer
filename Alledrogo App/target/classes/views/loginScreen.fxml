<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginController">
  <children>
    <AnchorPane>
      <children>
            <ImageView fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/LS.jpg" />
               </image></ImageView>
            <ImageView fitHeight="150.0" fitWidth="1000.0" layoutX="371.0" layoutY="85.0" pickOnBounds="true" preserveRatio="true" AnchorPane.topAnchor="50.0">
               <image>
                  <Image url="@../images/logo.png" />
               </image>
            </ImageView>
            <Button fx:id="loginButton" layoutX="440.0" layoutY="297.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="400.0" style="-fx-background-color: #757575; -fx-text-fill: #FFFFFF;" text="Zaloguj się" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Button>
            <Button fx:id="createButton" layoutX="440.0" layoutY="360.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="400.0" style="-fx-background-color: #757575; -fx-text-fill: #FFFFFF;" text="Utwórz nowe konto" textAlignment="CENTER" AnchorPane.topAnchor="360.0">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Button>
      </children>
    </AnchorPane>
  </children>
</VBox>
